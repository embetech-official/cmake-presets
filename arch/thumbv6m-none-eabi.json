{
    "version": 6,
    "include": [
        "../toolchain/gcc.json"
    ],
    "configurePresets": [
        {
            "name": "thumbv6m-none-eabi",
            "displayName": "arch: ARMv6-M, os: None, fp: soft, toolchain: gcc",
            "description": "ARMv6-M for bare metal (Cortex M0, M0+, M1)",
            "inherits": "gcc-arm-none-eabi",
            "cacheVariables": {
                "CMAKE_LIBRARY_ARCHITECTURE": "thumbv6m-none-eabi"
            },
            "environment": {
                "GCC_ARCHITECTURE": "-mthumb -march=armv6-m -mfloat-abi=soft"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "thumbv6m-none-eabi",
            "description": "No build targets specified",
            "configurePreset": "thumbv6m-none-eabi"
        }
    ]
}