{
    "version": 6,
    "include": [
        "../toolchain/gcc.json"
    ],
    "configurePresets": [
        {
            "name": "armv6-linux-gnueabihf",
            "displayName": "arch: ARMv6,        os: Linux, fp: hard",
            "description": "AArch32 for Linux",
            "inherits": "gcc-arm-none-linux-gnueabihf",
            "cacheVariables": {
                "CMAKE_LIBRARY_ARCHITECTURE": "armv6-linux-gnueabihf"
            },
            "environment": {
                "ARCHITECTURE": "-marm -march=armv6+fp -mfloat-abi=hard"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "armv6-linux-gnueabihf",
            "description": "No build targets specified",
            "configurePreset": "armv6-linux-gnueabihf"
        }
    ]
}