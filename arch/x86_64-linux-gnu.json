{
    "version": 6,
    "include": [
        "../toolchain/gcc.json"
    ],
    "configurePresets": [
        {
            "name": "x86_64-linux-gnu",
            "displayName": "arch: x86_64,        os: Linux, fp: hard",
            "description": "x86_64 for Linux",
            "inherits": "gcc-x86_64-linux-gnu",
            "cacheVariables": {
                "CMAKE_LIBRARY_ARCHITECTURE": "x86_64-linux-gnu"
            },
            "environment": {
                "ARCHITECTURE": "-march=x86-64"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "x86_64-linux-gnu",
            "description": "No build targets specified",
            "configurePreset": "x86_64-linux-gnu"
        }
    ]
}