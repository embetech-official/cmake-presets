{
    "version": 6,
    "include": [
        "${fileDir}/../../toolchain/gcc.json"
    ],
    "configurePresets": [
        {
            "name": "x86_64-linux-gnu",
            "displayName": "arch: x86_64,        os: Linux, fp: hard",
            "description": "x86_64 for Linux",
            "inherits": "gcc-x86_64-linux-gnu",
            "cacheVariables": {
                "CMAKE_LIBRARY_ARCHITECTURE": "x86_64-linux-gnu",
                "CMAKE_CXX_FLAGS_INIT": "-march=x86-64 -ffunction-sections -fdata-sections",
                "CMAKE_C_FLAGS_INIT": "-march=x86-64 -ffunction-sections -fdata-sections",
                "CMAKE_ASM_FLAGS_INIT": "-march=x86-64 -x assembler-with-cpp",
                "CMAKE_EXE_LINKER_FLAGS_INIT": "-Wl,--gc-sections"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "x86_64-linux-gnu",
            "description": "No build targets specified",
            "configurePreset": "x86_64-linux-gnu"
        }
    ]
}