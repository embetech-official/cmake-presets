{
    "version": 6,
    "include": [
        "${fileDir}/../../toolchain/msvc.json"
    ],
    "configurePresets": [
        {
            "name": "x86_64-windows-msvc",
            "displayName": "arch: x86_64, os: Windows, fp: hard, toolchain: msvc",
            "description": "x86_64 for Windows (MSVC)",
            "inherits": "msvc-x86_64",
            "cacheVariables": {
                "CMAKE_LIBRARY_ARCHITECTURE": "x86_64-windows-msvc"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "x86_64-windows-msvc:debug",
            "description": "Debug - No build targets specified",
            "configurePreset": "x86_64-windows-msvc",
            "configuration": "Debug"
        },
        {
            "name": "x86_64_windows-msvc:release",
            "description": "Release - No build targets specified",
            "configurePreset": "x86_64-windows-msvc",
            "configuration": "Release"
        }
    ]
}