{
    "version": 6,
    "include": [
        "../toolchain/gcc.json"
    ],
    "configurePresets": [
        {
            "name": "thumbv8m.base-none-eabi",
            "displayName": "arch: ARMv8-M baseline, os: None, fp: soft",
            "description": "ARMv8-M for bare metal (Cortex M23)",
            "inherits": "gcc-arm-none-eabi",
            "environment": {
                "ARCHITECTURE": "-mthumb -march=armv8-m.base -mfloat-abi=soft"
            }
        }
    ],
    "buildPresets": [
        {
            "name": "thumbv8m.base-none-eabi",
            "description": "No build targets specified",
            "configurePreset": "thumbv8m.base-none-eabi"
        }
    ]
}